-----------------------------------------//
Asignar tramites

url: localhost:8000/resol_tramites/asignar/:id_user
post
enviar:
{
	"id_tramite": 1,
	"id_usuario_asignado": 1,
	"respuesta": "Gracias por participar"
}

resouesta:
{
    "success": true,
    "msg": "Se asigno con exito"
}

----------------------------------------

Responder tramites

url: localhost:8000/resol_tramites/responder/:id_user
post
enviar:
{
	"id_asignacion_tramite": 1,
	"respuesta": "Gracias por participar"
}

resouesta:
{
    "success": true,
    "msg": "Se asigno con exito"
}

