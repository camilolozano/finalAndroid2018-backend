==========   CRUD clientes

metodo: get
Traer informaci贸n del cliente
url: http://localhost:8000/cliente//:id_user&:id_cli

regresa:{
  success: true,
  data: : {
    id_cliente: 'result.id_cliente',
    nombre1: 'result.nombre1',
    nombre2: 'result.nombre2',
    apellido1: 'result.apellido1',
    apellido2: 'result.apellido1',
    id_tipo_identificacion: 'result.id_tipo_identificacion',
    identificacion: 'result.identificacion',
    email: 'result.correo',
    direccion: 'result.direccion'
  }
}

metodo: get
Traer informaci贸n del cliente
url: http://localhost:8000/cliente/:id_user&:id_cli

regresa:{
  success: true,
  data: : {
    id_cliente: 'result.id_cliente',
    nombre1: 'result.nombre1',
    nombre2: 'result.nombre2',
    apellido1: 'result.apellido1',
    apellido2: 'result.apellido1',
    id_tipo_identificacion: 'result.id_tipo_identificacion',
    identificacion: 'result.identificacion',
    email: 'result.correo',
    direccion: 'result.direccion'
  }
}


metodo: post
crear del cliente
url: http://localhost:8000/cliente/create/:id_user

regresa:{
  success: true | false,
  msg: 'Cliente creado exitosamente | Error al guardar',
  data: {
    id_cliente: 'result.id_cliente',
    nombre1: 'result.nombre1',
    nombre2: 'result.nombre2',
    apellido1: 'result.apellido1',
    apellido2: 'result.apellido1',
    id_tipo_identificacion: 'result.id_tipo_identificacion',
    identificacion: 'result.identificacion',
    email: 'result.correo',
    direccion: 'result.direccion'
  }
}

metodo: put
Actualizar cliente
url: http://localhost:8000/cliente/update/:id_user

regresa:{
  success: true | false,
  msg: 'Cliente actualizado exitosamente | Error al actualizar',
}