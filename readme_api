--------------------------------------
Creacion de tramites
url: localhost:8000/crear_reporte/create/:id_user
post
enviar:
// Reportante nuevo
{
  "id_tipo_tramite": 1,
  "id_user": 1,
  "cedula_reportante": "231231231",
  "nombre1_reportante": "pedro",
  "nombre2_reportante" : "pedro",
  "apellido1_reportante": "perez",
  "apellido2_reportante": "perez",
  "direccion_reportante": "calle 123123",
  "numero_reportante": "12123123",
  "id_tipo_registro" : 1,
  "id_asunto" : 2,
  "observacion": "Observaciones",
  "id_informacion_titular": 1
}
// Reportante con id_asunto
{
  "id_tipo_registro":1,
  "id_asunto":3,
  "id_reportante": 3,
  "observacion": "Esta es la observacion del daño",
  "id_informacion_titular": 2,
  "id_tipo_tramite":1
}

